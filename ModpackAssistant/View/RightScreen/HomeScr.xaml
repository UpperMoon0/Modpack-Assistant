<UserControl x:Class="ModpackAssistant.View.RightScreen.HomeScr"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ModpackAssistant.View.RightScreen"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Rectangle Grid.Row="0" Fill="#333333" Height="Auto" Width="Auto"/>
        <Button Content="Create new project" Command="{Binding CreatePrjCmd}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="15,15,0,0" Width="120" Height="30"/>
    </Grid>
</UserControl>
